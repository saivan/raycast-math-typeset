{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "asciimath",
  "title": "Asciimath",
  "description": "Renders an asciimath equation",
  "icon": "command-icon.png",
  "author": "saivan",
  "categories": [
    "Data",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Asciimath",
      "subtitle": "Render an equation",
      "description": "Renders an equation to asciimath",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.1",
    "asciimath-to-latex": "^0.5.1",
    "express": "^4.18.2",
    "https": "^1.0.0",
    "jsdom": "^20.0.0",
    "katex": "^0.16.0",
    "mathjax": "^3.2.2",
    "mathjax-full": "^3.2.2",
    "mathjax-node": "^2.1.1",
    "node-fetch": "^3.3.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/katex": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
